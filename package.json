{
  "name": "nightscout-discord-rich-presence",
  "version": "2.0.0",
  "description": "Allows nightscout to display your blood sugar on discord.",
  "main": "build/index.js",
  "repository": "git@github.com:legoandmars/nightscout-discord-rich-presence.git",
  "author": "legoandmars/Bobbie",
  "contributors": [
    "Jack Baron <jackmbaron@gmail.com> (https://www.jackbaron.com)"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "yarn run check && yarn run lint",
    "check": "yarn run build --noEmit",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prebuild": "rimraf build",
    "build": "tsc",
    "build:dev": "yarn run build --sourceMap",
    "watch": "tsc --watch",
    "dev": "yarn run watch --sourceMap"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "@types/is-url": "^1.2.28",
    "@types/node": "^12.7.8",
    "@types/prompts": "^2.0.1",
    "@types/yaml": "^1.0.2",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "is-url": "^1.2.4",
    "kleur": "^3.0.3",
    "prompts": "^2.2.1",
    "read-pkg": "^5.2.0",
    "source-map-support": "^0.5.13",
    "yaml": "^1.7.0"
  }
}
